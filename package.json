{
    "name": "hackerlog",
    "displayName": "hackerlog",
    "description": "",
    "version": "0.0.1",
    "publisher": "dericcain",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./dist/index",
    "scripts": {
        "vscode:prepublish": "yarn build",
        "build": "tsc -p ./",
        "start": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "yarn build && node ./node_modules/vscode/bin/test",
        "lint": "tslint -p tslint.json",
        "lint:fix": "tslint -p tslint.json --fix"
    },
    "devDependencies": {
        "@types/jest": "^23.1.4",
        "@types/node": "^7.0.43",
        "jest": "^23.3.0",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "adm-zip": "^0.4.11",
        "lodash": "^4.17.10",
        "request": "^2.87.0"
    },
    "contributes": {
        "commands": [
            {
                "command": "hackerlog.editorKey",
                "title": "Hackerlog Editor Key"
            },
            {
                "command": "hackerlog.proxy",
                "title": "Hackerlog Proxy"
            },
            {
                "command": "hackerlog.debug",
                "title": "Hackerlog Debug"
            },
            {
                "command": "hackerlog.statusBarIcon",
                "title": "Hackerlog Status Bar Icon"
            },
            {
                "command": "hackerlog.dashboard",
                "title": "Hackerlog Dashboard"
            }
        ]
    }
}
