{
	"extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
	"rules": {
		"no-string-throw": true,
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"object-literal-sort-keys": false,
		"ordered-imports": false,
		"curly": true,
		"class-name": true,
		"semicolon": [
			true,
			"always"
		],
		"triple-equals": true
	},
	"defaultSeverity": "warning"
}